{
  "corsRules": [
    {
      "corsRuleName": "Allow web app access",
      "allowedOrigins": [
        "http://localhost:5173",
        "http://localhost:3000"
      ],
      "allowedOperations": [
        "b2_download_file_by_id",
        "b2_download_file_by_name",
        "b2_upload_file",
        "b2_upload_part",
        "b2_list_file_names",
        "b2_list_file_versions"
      ],
      "allowedHeaders": [
        "authorization",
        "content-type",
        "x-bz-file-name",
        "x-bz-content-sha1",
        "x-bz-info-*"
      ],
      "exposeHeaders": [
        "x-bz-content-sha1"
      ],
      "maxAgeSeconds": 3600
    }
  ]
}
